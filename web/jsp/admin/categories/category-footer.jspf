<%@ page contentType="text/html;charset=UTF-8"%>
<script>
    const btnAdd = document.getElementById("add-category");
    btnAdd.onclick = function () {
        window.location.href = "./edit-category?id=0";
    }

    const btnsEdit = document.getElementsByClassName("edit-link");
    for (let i = 0; i < btnsEdit.length; i++) {
        btnsEdit[i].setAttribute("title", "<fmt:message key="admin.categories.control.edit"/>")
    }
</script>